drop table sightings;
CREATE TABLE sightings (
    sighting_id INTEGER PRIMARY KEY AUTOINCREMENT,
    sighting_date TEXT,
    url TEXT,
    user_id INTEGER NOT NULL,
    museum_name TEXT,
    title TEXT,
    artist TEXT,
    date_created TEXT,
    FOREIGN KEY (user_id) REFERENCES user (id)
);
.read server/queries/insertSightings.sql